{
  "name": "What does the markup in your page, on average, mean?",
  "version": "0.1",
  "description": "You keep using that word. I do not think it means what you think it means.",
  "background": {
    "scripts": [ "meaningless.js", "background.js" ]
  },
  "content_scripts": [
    {
      "run_at": "document_idle",
      "matches": [ "http://*/*", "https://*/*" ],
      "all_frames": true,
      "css": [ "meaningless.css" ],
      "js":  [ "meaningless.js", "content.js" ]
    }
  ],
  "manifest_version": 2
}